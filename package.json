{
  "name": "@azu/packemon-demo",
  "version": "1.0.2",
  "description": "Packemon demo package",
  "keywords": [
    "Packemon",
    "npm",
    "typescript"
  ],
  "repository": "azu/packemon-demo",
  "license": "MIT",
  "author": "azu",
  "type": "commonjs",
  "main": "./cjs/index.cjs",
  "module": "./mjs/index.mjs",
  "types": "",
  "bin": "./cjs/bin.cjs",
  "files": [
    "cjs/**/*.{cjs,map}",
    "dts/**/*.d.ts",
    "mjs/**/*.{mjs,map}",
    "src/**/*.{ts,tsx,json}"
  ],
  "scripts": {
    "build": "packemon build --declaration standard",
    "test": "npm run type-check && npm run pack-check",
    "pack-check": "packemon pack --declaration standard && packemon validate",
    "type-check": "tsc --noEmit",
    "clean": "packemon clean"
  },
  "dependencies": {},
  "devDependencies": {
    "packemon": "^1.4.1",
    "typescript": "^4.4.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "packemon": {
    "inputs": {
      "bin": "src/bin.ts"
    },
    "format": [
      "cjs",
      "mjs"
    ],
    "platform": "node"
  }
}

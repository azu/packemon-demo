  },
  "dependencies": {},
  "devDependencies": {
    "packemon": "^1.4.1",
    "typescript": "^4.4.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "packemon": {
    "format": "mjs",
    "platform": "node"
  }

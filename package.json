{
  "name": "@azu/packemon-demo",
  "version": "1.0.0",
  "description": "Packemon demo package",
  "keywords": [
    "Packemon",
    "npm",
    "typescript"
  ],
  "license": "MIT",
  "author": "azu",
  "type": "module",
  "main": "./mjs/index.mjs",
  "module": "./mjs/index.mjs",
  "types": "./dts/index.d.ts",
  "files": [
    "dts/**/*.d.ts",
    "mjs/**/*.{mjs,map}",
    "src/**/*.{ts,tsx,json}"
